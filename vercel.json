{
   "version": 2,
   "name": "music-album",
   "builds": [
     { "src": "app.js", "use": "@vercel/node" }
   ],
   "routes": [
     {
       "src": "/login", 
       "methods": ["POST"],
       "dest": "/app.js",
       "headers": {
         "api": {
           "timeout": 30 
         }
       }
     },
     {
       "src": "/register", 
       "methods": ["POST"],
       "dest": "/app.js",
       "headers": {
         "api": {
           "timeout": 20 
         }
       }
     },
     {
       "src": "/(.*)", 
       "dest": "/app.js"
     }
   ]
 }
 